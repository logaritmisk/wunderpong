<?php
/**
 * @file
 * Code for the Statistics feature.
 */

include_once 'wp_statistics.features.inc';


/**
 * Implements hook_ctools_plugin_directory()
 */
function wp_statistics_ctools_plugin_directory($module, $type) {
  if (in_array($type, array('content_types'))) {
    return "plugins/$type";
  }
}
